Nutrients <- read.csv("~/Desktop/Classes/MARI 301/MARI301.nutrients.2017.csv", header=TRUE)
OxygenLight <- read.csv("~/Desktop/Classes/MARI 301/oxygenlight(2).csv", header=TRUE)
