O2FluxRaw[is.na(O2FluxRaw)] <- 0


#StartRichness
StartRichness <- specnumber(StartMacrofauna[,-2])
EndRichness <- specnumber(EndMacrofauna[,-2])


#Diversity
StartDiversity <- diversity(StartMacrofauna[,-2])
EndDiversity <- diversity(EndMacrofauna[,-2])


